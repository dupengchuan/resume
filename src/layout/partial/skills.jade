section#skills
  div.content
    +title('skills')
    each skill in skills
      div.skill-list
        h2.type!= skills_type[skill.type]
        each item in skill.item
          div.skill-item
            if item.summary != ""
              h3.skill-title
                != item.summary
            each desc in item.description
              p.desc!= desc
          
